# Test Environment Configuration
# Copy this to .env.test and fill in your values

# Test user credentials
TEST_USERNAME=testuser
TEST_EMAIL=test@example.com

# JWT token for test user (generate via your API or manually)
# This token should have a long expiry (30+ days)
TEST_TOKEN=your-jwt-token-here

# Optional: Base URL for tests (defaults to http://localhost:5173)
# VITE_API_URL=http://localhost:5173

# Optional: Backend API URL (if different from frontend)
# API_BASE_URL=http://localhost:8000
